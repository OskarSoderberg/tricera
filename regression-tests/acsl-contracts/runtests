#!/bin/sh

LAZABS=../../tri

TESTS="contract1.hcc \
       contract2.hcc \
       contract3.hcc \
       contract4.hcc \
       contract5.hcc \
       contract6.hcc \
       contract7.hcc \
       contract8.hcc \
       contract9.hcc \
       contract10.hcc"

for name in $TESTS; do
    echo
    echo $name
    $LAZABS "$@" $name 2>&1 | grep -v 'at '
done

